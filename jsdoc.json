{
    "plugins": ["plugins/markdown"],
    "source": {
        "include" : ["./src/js"],
        "includePattern": ".js",
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
    "destination": "./jsdoc",
    "recurse": true,
    "readme":"./README.md"
    }
}